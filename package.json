    "autosize": "^6.0.1",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "embla-carousel": "^8.2.0",
    "embla-carousel-autoplay": "^8.2.0",
    "embla-carousel-react": "^8.2.0",
